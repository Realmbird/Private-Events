 <%= render "shared/navbar" %>
<h1>Index Events</h1>
<% @events.each do |event|%>
  <h2><%= event.name %></h3>
  <h3><%= event.location%></h3>
  <h3><%= event.date %></h3>
  <h3><%= link_to "Edit", edit_event_path(event)%></h3>
  <h3><%= link_to "RSPV", edit_invitation_path(event)%></h3>
<% end %>

