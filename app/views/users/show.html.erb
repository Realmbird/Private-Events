<h1>User Created Events: </h1>
<% @user.events.each do |event| %>
  <h2><%= event.name%></h3>
  <h3><%= event.location%></h3>
  <h3><%= event.date %></h3>
<%end%>
<h2>Attended Events</h2>
<% @user.attended_events.each do |event|%>
  <h2><%= event.name%></h3>
  <h3><%= event.location%></h3>
  <h3><%= event.date %></h3>
<% end %>